{
  "name": "C++",
  "require_compile": true,
  "source_ext": ".cpp",
  "exec_name": "{0}",
  "compile_cmd": ["g++","-O2","-DEVAL","-pipe","-static","-s","{arg}", "{0}", "-o", "{1}"],
  "compile_runner_cmd": ["g++","-O2","-DEVAL","-pipe","-static","-s","{arg}", "{0}", "{2}", "-o", "{3}"],
  "exec_cmd": ["{0}"],
  "branches": {
    "C++11": {
      "arg": "-std=c++11"
    },
    "C++14": {
      "arg": "-std=c++14"
    },
    "C++17": {
      "arg": "-std=c++17"
    },
    "C++20": {
      "arg": "-std=c++20"
    }
  },
  "default_branch": "C++17"
}