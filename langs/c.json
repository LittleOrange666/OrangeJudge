{
  "name": "C",
  "require_compile": true,
  "source_ext": ".c",
  "exec_name": "{0}",
  "compile_cmd": ["gcc","-O2","-w","-fmax-errors=3","-lm","{arg}", "{0}", "-o", "{1}"],
  "compile_runner_cmd": ["gcc","-O2","-w","-fmax-errors=3","-lm","{arg}", "{0}", "{2}", "-o", "{3}"],
  "exec_cmd": ["{0}"],
  "branches": {
    "C99": {
      "arg": "-std=c99"
    },
    "C11": {
      "arg": "-std=c11"
    },
    "C17": {
      "arg": "-std=c17"
    }
  },
  "default_branch": "C99",
  "seccomp_rule": "c_cpp"
}